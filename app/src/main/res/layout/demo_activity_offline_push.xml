            <CheckBox
                android:id="@+id/checkbox25"
                style="@style/select_style"
                android:tag="26"
                android:text="@string/myf_tag25" />

        </LinearLayout>

        <LinearLayout
            android:scaleX="1.1"
            android:scaleY="1.1"
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout6"
            app:layout_constraintTop_toBottomOf="@id/linearLayout5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/linearLayout7"
            >

            <CheckBox
                android:id="@+id/checkbox26"
                style="@style/select_style"
                android:tag="27"
                android:text="@string/myf_tag26" />

            <CheckBox
                android:id="@+id/checkbox27"
                style="@style/select_style"
                android:tag="28"
                android:text="@string/myf_tag27" />

            <CheckBox
                android:id="@+id/checkbox28"
                style="@style/select_style"
                android:tag="29"
                android:text="@string/myf_tag28" />

            <CheckBox
                android:id="@+id/checkbox29"
                style="@style/select_style"
                android:tag="30"
                android:text="@string/myf_tag29" />

            <CheckBox
                android:id="@+id/checkbox30"
                style="@style/select_style"
                android:tag="31"
                android:text="@string/myf_tag30" />

        </LinearLayout>

        <LinearLayout
            android:scaleX="1.1"
            android:scaleY="1.1"
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="10dp"
            android:id="@+id/linearLayout7"
            app:layout_constraintTop_toBottomOf="@id/linearLayout6"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/cb_select"
            >

            <CheckBox
                android:id="@+id/checkbox31"
                style="@style/select_style"
                android:tag="32"
                android:text="@string/myf_tag31" />


        </LinearLayout>




        <Button
            android:id="@+id/btn_save"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:layout_marginLeft="15dip"
            android:layout_marginRight="15dip"
            android:layout_marginTop="20dip"
            android:textColor="#fff"
            android:text="@string/save"
            android:background="@drawable/demo_button_login_bg" />
    </LinearLayout>
</LinearLayout>